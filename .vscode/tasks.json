{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "build test for note.c",
            "command": "D:\\installed\\mingw64\\bin\\gcc.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-std=c11",
                "-I",
                "${default}",
                "-g",
                "test_note.c",
                "note.c",
                "-o",
                "${workspaceFolder}\\testbuild\\test_note.exe"
            ],
            "options": {
                "cwd": "${fileDirname}",
                "env": {
                    "TMP": "${workspaceFolder}/temp"
                }
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": false
            },
            "detail": "调试器生成的任务。"
        },
        {
            "label": "run test for note.c",
            "type": "shell",
            "command": "${workspaceFolder}\\testbuild\\test_note.exe",
            "dependsOn": [
                "build test for note.c"
            ],
            "group": {
                "kind": "test",
                "isDefault": false
            }
        },
        {
            "type": "cppbuild",
            "label": "build test for audio synthesizer",
            "command": "D:\\installed\\mingw64\\bin\\gcc.exe",
            "args": [
                "-fdiagnostics-color=always",
                "-std=c11",
                "-I",
                "${default}",
                "-g",
                "test_asynthesis.c",
                "note.c",
                "asynthesis.c",
                "${workspaceFolder}\\src\\unity\\unity.c",
                "-o",
                "${workspaceFolder}\\testbuild\\test_synthesizer.exe"
            ],
            "options": {
                "cwd": "${fileDirname}",
                "env": {
                    "TMP": "${workspaceFolder}/temp"
                }
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": false
            },
            "detail": "调试器生成的任务。"
        },
        {
            "label": "run test for audio synthesizer",
            "type": "shell",
            "command": "${workspaceFolder}\\testbuild\\test_synthesizer.exe",
            "dependsOn": [
                "build test for audio synthesizer"
            ],
            "group": {
                "kind": "test",
                "isDefault": false
            }
        }
    ],
    "version": "2.0.0"
}